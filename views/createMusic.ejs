<html lang="en">

<head>
  <meta charset="UTF-8">
  <title>Create Music</title>
  <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css" />
  <script src="https://code.jquery.com/jquery-2.2.0.min.js"></script>
  <script>function redirect(){window.location="/";}</script>
  <% include navbar %>
</head>

<body>
  <div>
    <% include selectItemType %>
  </div>
  <div class="container col-sm-12" id="mainform">
    <div id="createMusic" style=" margin-top:50px" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">

      <div class="panel panel-info">
        <div class="panel-heading">
          <div class="panel-title">Create Music</div>
        </div>
        <div class="panel-body">
          <form class="form-horizontal" role="form" method="post" action="/signup" onSubmit="return checkblank(this);">

            <div>
              <div class="form-group">
                <label for="title" class=" col-md-3 control-label"> Title </label>
                <div class="col-md-9">
                  <input type="number" class="form-control" name="movieid" placeholder=" ID of the movie">
                </div>
              </div>
              <div class="form-group">
                <label for="mob_no" class="col-md-3 control-label"> Artist </label>
                <div class="col-md-9">
                  <input type="artist" class="form-control" name="artist" placeholder="Name of the Artist">
                </div>
              </div>
              <div class="form-group">
                <label for="mob_no" class="col-md-3 control-label"> Label </label>
                <div class="col-md-9">
                  <input type="text" class="form-control" name="label" placeholder="Label of Music">
                </div>
              </div>
              <div class="form-group">
                <label for="mob_no" class="col-md-3 control-label"> Release date </label>
                <div class="col-md-9">
                  <input type="date" class="form-control" name="releasedate" placeholder="Release date">
                </div>
              </div>
              <div class="form-group">
                <label for="password" class="col-md-3 control-label"> Language </label>
                <div class="col-md-9">
                  <input type="text" class="form-control" name="language" placeholder="Language of the Music">
                </div>
              </div>
              <div class="form-group">
                <label for="password" class="col-md-3 control-label"> music_id </label>
                <div class="col-md-9">
                  <input type="number" class="form-control" name="music_id" placeholder="ID of the Music">
                </div>
              </div>
              <div class="form-group">
                <label for="form-group" class="col-md-3 control-label"> Quantity </label>
                <div class="col-md-9">
                  <input type="number" class="form-control" name="quantity" placeholder="Quantity available">
                </div>
              </div>
            </div>

              <div class="form-group">
                <!-- Button -->
                <div class="col-md-offset-3 col-md-9">
                  <button id="btn-createMusic" type="submit" class="btn btn-info"><i class="icon-hand-right"></i>
                    &nbsp Create</button>
                </div>
              </div>
          </form>
        </div>
      </div>
    </div>
  </div>

</html>
