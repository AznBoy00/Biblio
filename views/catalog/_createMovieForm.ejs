<div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" name="title" placeholder="Title of the Movie">
</div>

<div class="form-group">
    <label>Director</label>
    <input type="text" class="form-control" name="director" placeholder="Director of the Movie">
</div>

<div class="form-group">
    <label>Producers</label>
    <input type="text" class="form-control" name="producers" placeholder="Producers of the Movie">
</div>

<div class="form-group">
    <label>Actors</label>
    <input type="text" class="form-control" name="actors" placeholder="Actors in the Movie (separated, by, commas)">
</div>

<div class="form-group">
    <label>Language</label>
    <input type="text" class="form-control" name="language" placeholder="Language of the Movie">
</div>

<div class="form-group">
    <label>Subtitles</label>
    <input type="text" class="form-control" name="subtitles" placeholder="Subtitles languages of the Movie">
</div>

<div class="form-group">
    <label>Dubbed</label>
    <input type="text" class="form-control" name="dubbed" placeholder="Dubbed languages of the Movie">
</div>

<div class="form-group">
    <label>Release Date</label>
    <input type="date" class="form-control" name="releasedate" placeholder="Release Date of the Movie">
</div>

<div class="form-group">
    <label>Run Time</label>
    <input type="number" class="form-control" name="runtime" placeholder="Run time of the Movie (in minutes)">
</div>

<div class="form-group">
    <label>Quantity</label>
    <input type="number" class="form-control" name="quantity" placeholder="Quantity available">
</div>