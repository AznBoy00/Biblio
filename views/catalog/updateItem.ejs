<!DOCTYPE html>
<html lang="en"> 

<head>
    <%- include('../partials/_header') -%>
    <script>function redirect(){window.location="/";}</script>
</head>

<body>
	<%- include('../partials/_navbar'); -%>
	<div class="container main">

		<div class="row">
			<div class="col-12 col-sm-10 offset-sm-1 col-md-8 offset-md-2">
                <h2>Update <%= discriminator %></h2>
			</div>
        </div>
        <div class="row">
            <div class="col-12 col-sm-10 offset-sm-1 col-md-8 offset-md-2">
                <form method="POST" action="/catalog/update/<%= results.results[0].item_id %>" onSubmit="return checkblank(this);">

                    <%- include('../partials/_error'); -%>
                    <% let pagename = 'partials/_update'+discriminator+'Form'; %>
                    <%- include(pagename); -%>
					<button class="btn  btn-sm btn-outline-success" type="submit">Submit</button>
					<a class="btn btn-sm btn-outline-danger" href="javascript:history.back()">Cancel</a>	
                </form>
            </div>
            <!-- ./ end col -->
        </div>
        <!-- ./ end row -->

    </div>
    <!-- ./ end container -->
    <%- include('../partials/_footer') -%>   
</body>

</html>