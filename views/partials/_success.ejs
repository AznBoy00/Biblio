<!-- Custom error messages -->
<!-- 
    Login, Register methods 
    From the Users Routes uses this
-->
<% if (typeof msg !== 'undefined') { %>
    <div class="alert alert-success" role="alert">
        <%= msg %>
    </div>
<% } %>

<!-- Default error messages -->
<!-- 
    Create items (book, magazine, music, movie)
    Update Items(book, magazine, music, movie)
    From the Catalog Routes uses this 
-->
<!-- Errors returns [location, param, msg, value] -->
<% if (typeof success !== 'undefined') { %>
    <div class="alert alert-success alert-dismissible" role="alert">
        <!-- <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button> -->
        <!-- <h3 class="my-1">Success</h3> -->
        <% for (var i = 0 ; i < success.length ; i++){%>
            <p class="my-3"><%= success[i] %></p>
        <% } %>
    </div>
<% } %>

