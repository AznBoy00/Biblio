<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>Create Item</title>
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css" />
    <script src="https://code.jquery.com/jquery-2.2.0.min.js"></script>
    <% include navbar %>
</head>

<body>
    <div class="container col-sm-12" id="mainform">
        <div id="signupbox" style=" margin-top:50px" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">

            <div class="panel panel-info">
                <div class="panel-heading">
                    <div class="panel-title">Create Book</div>
                </div>
                <div class="panel-body">
                    <form class="form-horizontal" role="form" method="post" action="/signup" onSubmit="return checkblank(this);">
                    
                    <div>
                        <div class="form-group">
                            <label for="first_name" class="col-md-3 control-label"> Title </label>
                            <div class="col-md-9">
                                <input type="text" class="form-control" name="title" placeholder=" Title of the Book">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="mob_no" class="col-md-3 control-label"> Author </label>
                            <div class="col-md-9">
                                <input type="author" class="form-control" name="author" placeholder="Name of the Author">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="mob_no" class="col-md-3 control-label"> Format </label>
                            <div class="col-md-9">
                                <input type="text" class="form-control" name="format" placeholder="Format of Book">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="mob_no" class="col-md-3 control-label"> Pages </label>
                            <div class="col-md-9">
                                <input type="number" class="form-control" name="pages" placeholder="Number of Pages">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="mob_no" class="col-md-3 control-label"> Publisher </label>
                            <div class="col-md-9">
                                <input type="text" class="form-control" name="publisher" placeholder="Publisher of the Book">
                            </div>
                        </div>
                        <div class="form-group">
                                <label for="password" class="col-md-3 control-label"> Language </label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" name="language" placeholder="Language of the Book">
                                </div>
                            </div>
                        <div class="form-group">
                            <label for="password" class="col-md-3 control-label"> ISBN10 </label>
                            <div class="col-md-9">
                                <input type="number" class="form-control" name="isbn10" placeholder="ISBN10 of the Book">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="password" class="col-md-3 control-label"> ISBN13 </label>
                            <div class="col-md-9">
                                <input type="number" class="form-control" name="isbn13" placeholder="ISBN13 of the Book">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="password" class="col-md-3 control-label"> Quantity </label>
                            <div class="col-md-9">
                                <input type="number" class="form-control" name="quantity" placeholder="Quantity available">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="password" class="col-md-3 control-label"> Loan Period </label>
                            <div class="col-md-9">
                                <input type="number" class="form-control" name="loadperiod" placeholder="Loan Period allocated">
                            </div>
                        </div>
                    </div>

                        <div class="form-group">
                            <!-- Button -->
                            <div class="col-md-offset-3 col-md-9">
                                <button id="btn-signup" type="submit" class="btn btn-info"><i class="icon-hand-right"></i>
                                    &nbsp Create</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

